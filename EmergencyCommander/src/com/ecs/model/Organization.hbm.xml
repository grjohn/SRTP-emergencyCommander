<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	 "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
	 
<hibernate-mapping>

	<class name="com.ecs.model.Organization" table="organization">
		<id name="id" column="id" type="string"></id>
		<property name="name" column="name" type="string" length="30"></property>
		<property name="isleaf" column="isleaf" type="boolean"></property>
		<property name="iconCls" column="iconCls" type="string"></property>
		<property name="pid" column="pid" type="string"></property>
		
		
	</class>

</hibernate-mapping>
