<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	 "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
	 
<hibernate-mapping>

	<class name="com.ecs.model.Provision" table="provision">
		<id name="id" column="id" type="integer">
			<generator class="native"></generator>
		</id>
		<property name="time" column="time" type="timestamp"></property>
		<property name="title" column="title" type="string" length="50"></property>
		<property name="confirmer" column="confirmer" type="string" length="20"></property>
		<property name="comment" column="comment" type="string" length="40"></property>
		<property name="filelocation" column="filelocation" type="string" length="100"></property>
		<property name="etime" column="etime" type="timestamp"></property>
		
		
	</class>

</hibernate-mapping>
