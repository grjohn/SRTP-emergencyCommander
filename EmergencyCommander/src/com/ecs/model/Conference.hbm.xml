<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	 "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
	 
<hibernate-mapping>

	<class name="com.ecs.model.Conference" table="conference">
		<id name="id" column="id" type="integer">
			<generator class="native"></generator>
		</id>
		<property name="name" column="name" type="string" length="40"></property>
		<property name="time" column="time" type="timestamp"></property>
		<property name="etime" column="etime" type="timestamp"></property>
		<property name="type" column="type" type="string"></property>
		<property name="leader" column="leader" type="string" length="40"></property>
		<property name="present" column="present" type="string"></property>
		<property name="detail" column="detail" type="text" ></property>
		<property name="address" column="address" type="string" length="50"></property>
		<property name="location" column="location" type="string" length="100"></property>
	</class>

</hibernate-mapping>
