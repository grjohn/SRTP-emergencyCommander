<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
	
<hibernate-mapping>

	<class name="com.ecs.model.Contact" table="contact">
		<id name="id" column="id" type="string">
		</id>
		<property name="name" column="name" type="string" length="40"></property>
		<property name="contact" column="contact" type="string" length="30"></property>
		<property name="ctel" column="ctel" type="string" length="30"></property>
		<property name="dtel" column="dtel" type="string" length="30"></property>
		<property name="email" column="email" type="string" length="50"></property>
		<property name="address" column="address" type="string" length="100"></property>
		<property name="postalcode" column="postalcode" type="string" length="20"></property>
		<property name="intro" column="intro" type="string"></property>
		<property name="comment" column="comment" type="string"></property>
	</class>

</hibernate-mapping>
